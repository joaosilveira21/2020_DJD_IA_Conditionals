using System;

namespace roll
{
    class Program
    {

        static void Main(string[] args)
        {
            Random r = new Random();

            int rollP1 = r.Next(1, 7);
            int rollP2 = r.Next(1, 7);

            if (rollP1 > rollP2) 
            {
                Console.WriteLine("Player 1 got the item !!");
            }
            else 
            {
                Console.WriteLine("Player 2 got the item !!");
            }
        }
    }
}
